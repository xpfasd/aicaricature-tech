---
import BaseLayout from '../layouts/BaseLayout.astro';
import PageTemplate from '../components/PageTemplate.astro';
import { PAGE_META } from '../lib/site';

const page = PAGE_META.about;
---

<BaseLayout locale="en" pageId="about" title={page.title} description={page.description}>
  <PageTemplate locale="en" pageId="about" showGenerator={false} />
</BaseLayout>
