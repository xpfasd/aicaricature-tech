---
import BaseLayout from '../layouts/BaseLayout.astro';
import PageTemplate from '../components/PageTemplate.astro';
import { PAGE_META } from '../lib/site';

const page = PAGE_META.terms;
---

<BaseLayout locale="en" pageId="terms" title={page.title} description={page.description}>
  <PageTemplate locale="en" pageId="terms" showGenerator={false} />
</BaseLayout>
